	file = {Doupe and Kuhl - 1999 - BIRDSONG AND HUMAN SPEECH Common Themes and Mecha.pdf:/Users/jkosie/Zotero/storage/MKAF9MLH/Doupe and Kuhl - 1999 - BIRDSONG AND HUMAN SPEECH Common Themes and Mecha.pdf:application/pdf}
